<br>
<div class="center">
  <h1 class="big">Edit a Thing</h1>
  <%= form_for(@thing) do |f| %>
    <%= render 'shared/error_messages' %>

    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.label :image_url %>
    <%= f.text_field :image_url, class: 'form-control' %>
    <br>
    <%= f.submit "Submit Thing", class: "button" %>
  <% end %>
</div>
