<%= form_for(@thing) do |f| %>
  <%= render 'shared/error_messages' %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url, class: 'form-control' %>

  <%= f.submit "Submit Thing", class: "btn btn-primary" %>
<% end %>
