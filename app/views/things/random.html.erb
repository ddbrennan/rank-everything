<br>

<div class="center">
  <div class="qbox">
      <%= @thing.name %>
  </div><br>
  <div class="center">
    <img src=<%= @thing.image_url %>>
  </div>
</div>

<%= form_tag votes_path do %>
  <%= hidden_field :user, :vote, :value => "like" %>
  <%= image_submit_tag "thumb.png", class:"good_thumb", name:"like", value:"like" %>
<% end %>

<%= form_tag votes_path do %>
  <%= hidden_field :user, :vote, :value => "dislike" %>
  <%= image_submit_tag "thumb.png", class:"bad_thumb", name:"dislike", value:"dislike" %>
<% end %>
